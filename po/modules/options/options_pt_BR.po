#: src/modules/options/optw_channel.cpp:122
msgid "<center>Minimum value: <b>0 secs</b><br>Maximum value: <b>60 secs</b></center>"
msgstr "<center>Valor mínimo: <b>0 seg</b><br>Valor máximo: <b>86400 seg</b></center>"

#: src/modules/options/optw_ctcpfloodprotection.cpp:93
msgid "<center>Minimum value: <b>1 sec</b><br>Maximum value: <b>3600 sec</b></center>"
